/chetan/vue-js/section_16/src/pages/coaches/CoachDetail.vue"],"sourcesContent":["<template>\n  <section>\n    <section>\n      <base-card>\n        <h2>{{ fullName }}</h2>\n        <h3>${{ rate }}/hour</h3>\n      </base-card>\n    </section>\n\n    <section>\n      <base-card>\n        <header>\n          <h2>Interested? Reach out now!</h2>\n          <base-button link :to=\"contactLink\">Contact</base-button>\n        </header>\n        <router-view></router-view>\n      </base-card>\n    </section>\n\n    <section>\n      <base-card>\n        <base-badge\n          v-for=\"area in areas\"\n          :key=\"area\"\n          :type=\"area\"\n          :title=\"area\"\n        ></base-badge>\n        <p>{{ description }}</p>\n      </base-card>\n    </section>\n  </section>\n</template>\n\n<script>\nexport default {\n  props: [\"id\"],\n  data() {\n    return {\n      selectedCoach: null,\n    };\n  },\n  computed: {\n      fullName() {\n          return this.selectedCoach.firstName + ' ' + this.selectedCoach.lastName;\n      },\n      areas() {\n          return this.selectedCoach.areas;\n      },\n      rate() {\n          return this.selectedCoach.hourlyRate;\n      },\n      description() {\n          return this.selectedCoach.description;\n      },\n      contactLink() {\n          return this.$route.path + '/' + this.id + '/contact';\n      }\n  },\n  created() {\n    this.selectedCoach = this.$store.getters[\"coaches/coaches\"].find(\n      (coach) => coach.id === this.id\n    );\n  },\n};\n</script>\n"]}]}
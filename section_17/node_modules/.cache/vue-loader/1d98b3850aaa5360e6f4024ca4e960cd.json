lt {\n    emits: ['change-filter'],\n    data() {\n        return {\n            filters: {\n                frontend: true,\n                backend: true,\n                career: true\n            }\n        };\n    },\n    methods: {\n        setFilter(event) {\n            const inputId = event.target.id;\n            const isActive = event.target.checked;\n            const updatedFilters = {\n                ...this.filters,\n                [inputId]: isActive\n            };\n            this.filters = updatedFilters; \n            this.$emit('change-filter', updatedFilters);\n        }\n    }\n}\n</script>\n\n<style scoped>\nh2 {\n  margin: 0.5rem 0;\n}\n\n.filter-option {\n  margin-right: 1rem;\n}\n\n.filter-option label,\n.filter-option input {\n  vertical-align: middle;\n}\n\n.filter-option label {\n  margin-left: 0.25rem;\n}\n\n.filter-option.active label {\n  font-weight: bold;\n}\n</style>"]}]}
"sourcesContent":["<template>\n  <section>\n    <h2>Add a products</h2>\n    <form @submit.prevent=\"submitForm\">\n      <div>\n        <label for=\"title\">Title</label>\n        <input type=\"text\" id=\"title\" v-model=\"enteredTitle\" />\n      </div>\n      <div>\n        <label for=\"price\">Price</label>\n        <input type=\"number\" id=\"price\" v-model.number=\"enteredPrice\" />\n      </div>\n      <div>\n        <label for=\"description\">Description</label>\n        <textarea rows=\"5\" id=\"description\" v-model=\"enteredDescription\"></textarea>\n      </div>\n      <button>Add Product</button>\n    </form>\n  </section>\n</template>\n\n<script>\nimport { ref, inject } from 'vue';\n\nexport default {\n  setup() {\n    const addProduct = inject('addProduct');\n\n    const enteredTitle = ref('');\n    const enteredPrice = ref(null);\n    const enteredDescription = ref('');\n\n    function submitForm() {\n      addProduct({\n        title: enteredTitle,\n        description: enteredDescription,\n        price: enteredPrice,\n      });\n    }\n\n    return {\n      enteredTitle,\n      enteredPrice,\n      enteredDescription,\n      submitForm,\n    };\n  },\n};\n</script>\n\n<style scoped>\nsection {\n  margin: 3rem auto;\n  max-width: 40rem;\n  padding: 1rem;\n  border-radius: 12px;\n  box-shadow: 0 2px 8px rgba(0, 0, 0, 0.26);\n}\n\ninput,\nlabel,\ntextarea {\n  display: block;\n  width: 100%;\n}\n\nlabel {\n  font-weight: bold;\n}\n\ninput,\ntextarea {\n  font: inherit;\n  margin-bottom: 0.5rem;\n}\n\nbutton {\n  font: inherit;\n  background-color: #570080;\n  border: 1px solid #570080;\n  color: white;\n  padding: 0.5rem 1.5rem;\n  cursor: pointer;\n}\n\nbutton:hover,\nbutton:active {\n  background-color: #220031;\n  border-color: #220031;\n}\n</style>"]}]}